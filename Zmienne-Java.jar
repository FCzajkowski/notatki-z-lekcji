  
public class Mmain{
  public static void main(String[] args){
  /*
    W Javie wystepuje typizacja Statyczna. 
    Zmienne przypisujemy jako: 
    
    <typ danych> <nazwa> = <wartosc>; 
    */
    int a = 10; 
    /*
    albo 
    <typ danych> <nazwa>; 
    i pozniej inicjacja: 
    <nazwa> = <wartosc>
    */
    int b;
    b = 10;
    System.out.print("a: ");
    System.out.print(a);
    System.out.print("b: ");
    System.out.print(b);
  }
}
